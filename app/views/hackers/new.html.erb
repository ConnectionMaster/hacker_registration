<div class="col-md-6 col-md-offset-3">
  <h1 class="head">GE @ MHACKS</h1>

  <%= form_tag do |f| %>

    <div class="form-group row">
      <%= text_field_tag 'name', nil, :class => 'form-control', :placeholder => 'Name' %>
    </div>

    <div class="form-group row">
      <%= text_field_tag 'github_id', nil, :class => 'form-control', :placeholder => 'GitHub name' %>
    </div>

    <button type="submit" class="btn btn-lg btn-success">Submit Profile</button>

  <% end %>
</div>
